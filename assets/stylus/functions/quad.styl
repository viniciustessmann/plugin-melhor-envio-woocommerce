// quad( color )
// returns quad (tetrad) variations of given color

me-quad( color )
    first = spin( color, 90 )
    second = spin( color, -90 )
    third = spin( color, 180 )
    ( first second third )

quad = me-quad unless me-no-conflict
me-tetrad = me-quad
tetrad = me-quad unless me-no-conflict
